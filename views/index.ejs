<h2>Recently Added</h2>
<div>
    <% books.forEach(book => { %>
      <a href="/books/<%= book.id %>">
        <img height="150" width="100" src="<%= book.coverImagePath %>" alt="image">
      </a>
        <div><%= book.title %></div>
        <div><%= book.publishDate %></div>
    <% }) %>
  </div>
  